<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Al_Mulla_POC" parallel="tests" thread-count="1">

    <test name="Verify_currency_exchange_rate">
        <classes>
            <class name="web.tests.ExchangeRate.currency_exchange_rateTests">
                <methods>
                    <include name="verify_value_in_currency_exchange_rate"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Verify_headers_data">
        <classes>
            <class name="web.tests.HeadersTest.headers_test">
                <methods>
                    <include name="verify_data_in_header"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Verify_remit_online">
        <classes>
            <class name="web.tests.Remit_online_test.remit_online_test">
                <methods>
                    <include name="verify_remit_online"></include>
                </methods>
            </class>
        </classes>
    </test>


</suite>